{
    "manifest_version": 2,
    "name": "sushiscan-dl",
    "version": "2.0",
    "description": "Add a download button on sushiscan",
    "icons": {},
    "content_scripts": [
        {
            "matches": [
                "https://sushiscan.net/*/"
            ],
            "js": [
                "jspdf.umd.min.js",
                "jszip.min.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "https://sushiscan.net/*"
    ]
}